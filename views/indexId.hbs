<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>phonographs</title>
  </head>
  <body>
    <main>
      {{#each phone}}
      <h1>{{this.pet_name}}</h1>
      <img src="{{this.img}}" alt="phonograph">
      <p>{{this.created_at}}</p>
      <a href="/{{this.id}}">Look don't listen</a>
      {{/each}}
    </main>
  </body>
</html>
